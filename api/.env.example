# Environment
NODE_ENV=development

# Server
PORT=3000

# Database
DATABASE_URL=postgresql://postgres:password@localhost:5432/myproject

# CORS - Comma-separated list of allowed origins, or "*" for public API
CORS_ORIGINS=*

# Project identification (for logging)
PROJECT_ID=
PROJECT_NAME=myproject

# Critical logs webhook (optional - for project-manager integration)
LOG_WEBHOOK_URL=
LOG_WEBHOOK_SECRET=
LOG_DEDUP_MINUTES=5
